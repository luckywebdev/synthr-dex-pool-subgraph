{
  "name": "synthr-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/Synthrio/sysnthr-synthetix-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "prepare:avax": "mustache config/avax.json dexcandles-v2.template.yaml > dexcandles-v2.avax.yaml && mustache config/avax.json src/mappings/constants/index.template.ts > src/mappings/constants/index.ts",
    "prepare:fuji": "mustache config/fuji.json dexcandles-v2.template.yaml > dexcandles-v2.fuji.yaml && mustache config/fuji.json src/mappings/constants/index.template.ts > src/mappings/constants/index.ts",
    "prepare:arbgoerli": "mustache config/arbgoerli.json dexcandles-v2.template.yaml > dexcandles-v2.arbgoerli.yaml && mustache config/arbgoerli.json src/mappings/constants/index.template.ts > src/mappings/constants/index.ts",
    "prepare:arbitrum": "mustache config/arbitrum.json dexcandles-v2.template.yaml > dexcandles-v2.arbitrum.yaml && mustache config/arbitrum.json src/mappings/constants/index.template.ts > src/mappings/constants/index.ts",
    "prepare:bsc": "mustache config/bsc.json dexcandles-v2.template.yaml > dexcandles-v2.bsc.yaml && mustache config/bsc.json src/mappings/constants/index.template.ts > src/mappings/constants/index.ts",
    "prepare:chapel": "mustache config/chapel.json dexcandles-v2.template.yaml > dexcandles-v2.chapel.yaml && mustache config/chapel.json src/mappings/constants/index.template.ts > src/mappings/constants/index.ts",
    "codegen:avax": "graph codegen dexcandles-v2.avax.yaml",
    "codegen:fuji": "graph codegen dexcandles-v2.fuji.yaml",
    "codegen:arbgoerli": "graph codegen dexcandles-v2.arbgoerli.yaml",
    "codegen:arbitrum": "graph codegen dexcandles-v2.arbitrum.yaml",
    "codegen:bsc": "graph codegen dexcandles-v2.bsc.yaml",
    "codegen:chapel": "graph codegen dexcandles-v2.chapel.yaml",
    "build:avax": "graph build dexcandles-v2.avax.yaml",
    "build:fuji": "graph build dexcandles-v2.fuji.yaml",
    "build:arbgoerli": "graph build dexcandles-v2.arbgoerli.yaml",
    "build:arbitrum": "graph build dexcandles-v2.arbitrum.yaml",
    "build:bsc": "graph build dexcandles-v2.bsc.yaml",
    "build:chapel": "graph build dexcandles-v2.chapel.yaml",
    "deploy:avax": "graph deploy --node https://api.thegraph.com/deploy/ luckywebdev/synthr-dex-pool-avax dexcandles-v2.avax.yaml",
    "deploy:fuji": "graph deploy --node https://api.thegraph.com/deploy/ luckywebdev/synthr-dex-pool-fuji dexcandles-v2.fuji.yaml",
    "deploy:arbgoerli": "graph deploy --node https://api.thegraph.com/deploy/ luckywebdev/synthr-dex-pool-arbgoerli dexcandles-v2.arbgoerli.yaml",
    "deploy:arbitrum": "graph deploy --node https://api.thegraph.com/deploy/ luckywebdev/synthr-dex-pool-arbitrum dexcandles-v2.arbitrum.yaml",
    "deploy:bsc": "graph deploy --node https://api.thegraph.com/deploy/ luckywebdev/synthr-dex-pool-bnb dexcandles-v2.bsc.yaml",
    "deploy:chapel": "graph deploy --node https://api.thegraph.com/deploy/ luckywebdev/synthr-dex-pool-bnbtest dexcandles-v2.chapel.yaml",
    "create-local": "graph create --node http://localhost:8020/ luckywebdev/synthr-dex-pool-fuji",
    "remove-local": "graph remove --node http://localhost:8020/ luckywebdev/synthr-dex-pool-fuji",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 luckywebdev/synthr-dex-pool-fuji",
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "auth": "graph auth --debug hosted-service https://api.thegraph.com/deploy/ 798b2dde29dd4aa0b461f22ba2af3f41",
    "deploy": "graph deploy luckywebdev/synthr-dex-pool-mumbai --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-staging": "graph deploy $THE_GRAPH_GITHUB_USER/$THE_GRAPH_SUBGRAPH_NAME /Synthr --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/",
    "watch-local": "graph deploy graphprotocol/Synthr --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.58.0",
    "@graphprotocol/graph-ts": "^0.31.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "mustache": "^4.2.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
